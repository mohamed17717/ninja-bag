        "method": "POST",
        "description": "take an image, new width and return new thumbnail of this image",
        "limits": ["storage", "bandwidth", "requests"],
        "dataType": "form",

        "params": {
          "POST": [
            {
              "name": "image",
              "required": true,
              "type": "image",
              "description": "image that will be change its size"
            }, {
              "name": "size",
              "default": "220",
